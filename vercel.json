{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/frontend/inicio.html" 
    },
    {
      "src": "/buscar",
      "dest": "/frontend/buscarPartitura.html"
    },
    {
      "src": "/index",
      "dest": "/frontend/index.html"  
    },
    {
      "src": "/css/(.*)",
      "dest": "/frontend/css/$1"
    },
    {
      "src": "/imagenes/(.*)",
      "dest": "/frontend/imagenes/$1"
    },
    {
      "src": "/frontend/(.*)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/inicio.html" 
    }
  ]
}