{
  "version": 2,
  "public": true,
  "rewrites": [
    {
      "source": "/(css|imagenes|js|fonts|static|assets)/(.*)",
      "destination": "/frontend/$1/$2"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ],
  "redirects": [
    {
      "source": "/inicio",
      "destination": "/",
      "permanent": true
    }
  ]
}