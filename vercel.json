{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/css/(.*)",
      "destination": "/frontend/css/$1"
    },
    {
      "source": "/imagenes/(.*)",
      "destination": "/frontend/imagenes/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/frontend/js/$1"
    },
    {
      "source": "/frontend/(.*)",
      "destination": "/frontend/$1"
    },
    {
      "source": "/buscar",
      "destination": "/frontend/buscarPartitura.html"
    },
    {
      "source": "/subir",
      "destination": "/frontend/index.html"
    },
    {
      "source": "/",
      "destination": "/frontend/inicio.html"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/inicio.html"
    }
  ],
  "headers": [
    {
      "source": "/buscar",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}